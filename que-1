<!DOCTYPE html>
<html>
<body>
<p id="demo"></p>
<p id="demo1"></p>

<script>
class Employee
{
  constructor(id,nm,sa)
  {
    this.cid=id
    this.cname=nm
    this.sal=sa
    
  }
get id()
  {
    return this.cid
  }
  set id(x)
  {
    this.cid=x
  }
  get name()
  {
    return this.cname
  }
  set name(x)
  {
    this.cname=x
  }
  get salary()
  {
    return this.sal
  }
  set salary(x)
  {
    this.sal=x
  }
netsal(x)
{
  return x-10*x/100;
}
}
var p=new Employee("1","GITESHWARI WADBUDHE",45000)
var q=new Employee("2","AASHU WADBUDHE",25000)
document.getElementById("demo").innerHTML="id "+p.cid+" "+"Name "+p.cname+" "+"Salary "+p.sal+" "+"Net "+p.netsal(45000)
document.getElementById("demo1").innerHTML="id "+q.cid+" "+"Name "+q.cname+" "+"Salary "+q.sal+" "+"Net "+q.netsal(25000)

</script>
</body>
</html>
